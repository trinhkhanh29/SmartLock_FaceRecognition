  <script>
    // Bi·∫øn to√†n c·ª•c
    let currentStream = null;
    let cameras = [];
    let currentCameraIndex = 0;
    let pinCode = '';
    const maxPinLength = 4;

    // DOM Elements
    const videoElement = document.getElementById('videoElement');
    const cameraPlaceholder = document.getElementById('cameraPlaceholder');
    const cameraStatus = document.getElementById('cameraStatus');
    const cameraSelect = document.getElementById('cameraSelect');
    const startCameraBtn = document.getElementById('startCameraBtn');
    const switchCameraBtn = document.getElementById('switchCameraBtn');
    const fullscreenBtn = document.getElementById('fullscreenBtn');
    const refreshBtn = document.getElementById('refreshBtn');
    const collectBtn = document.getElementById('collectBtn');
    const pinDisplay = document.getElementById('pinDisplay');
    const toast = document.getElementById('toast');

    // Kh·ªüi t·∫°o
    document.addEventListener('DOMContentLoaded', function() {
      loadCameras();
      setupEventListeners();
    });

    // T·∫£i danh s√°ch camera
    async function loadCameras() {
      try {
        showToast('ƒêang t·∫£i danh s√°ch camera...', 'warning');
        
        const devices = await navigator.mediaDevices.enumerateDevices();
        cameras = devices.filter(device => device.kind === 'videoinput');
        
        cameraSelect.innerHTML = '<option value="">-- Ch·ªçn camera --</option>';
        
        if (cameras.length === 0) {
          cameraSelect.innerHTML = '<option value="">Kh√¥ng t√¨m th·∫•y camera</option>';
          showToast('Kh√¥ng t√¨m th·∫•y camera n√†o tr√™n thi·∫øt b·ªã', 'error');
          return;
        }
        
        cameras.forEach((camera, index) => {
          const label = camera.label || `Camera ${index + 1}`;
          const option = document.createElement('option');
          option.value = index;
          option.textContent = label;
          cameraSelect.appendChild(option);
        });
        
        showToast(`ƒê√£ t√¨m th·∫•y ${cameras.length} camera`, 'success');
        
      } catch (error) {
        console.error('L·ªói khi t·∫£i camera:', error);
        showToast('L·ªói khi t·∫£i danh s√°ch camera', 'error');
      }
    }

    // B·∫≠t camera
    async function startCamera(deviceId = null) {
      try {
        stopCamera(); // D·ª´ng camera hi·ªán t·∫°i n·∫øu c√≥
        
        const constraints = {
          video: {
            width: { ideal: 1280 },
            height: { ideal: 720 },
            facingMode: 'user'
          }
        };

        if (deviceId) {
          constraints.video.deviceId = { exact: deviceId };
        }

        const stream = await navigator.mediaDevices.getUserMedia(constraints);
        currentStream = stream;
        videoElement.srcObject = stream;
        
        // Hi·ªÉn th·ªã video v√† ·∫©n placeholder
        videoElement.style.display = 'block';
        cameraPlaceholder.style.display = 'none';
        cameraStatus.style.display = 'block';
        cameraStatus.textContent = 'üìπ Camera ƒëang ho·∫°t ƒë·ªông';
        cameraStatus.style.background = 'rgba(76, 175, 80, 0.8)';
        
        // C·∫≠p nh·∫≠t UI
        startCameraBtn.innerHTML = '<i class="fas fa-stop"></i> T·∫Øt Camera';
        startCameraBtn.classList.remove('btn-secondary');
        startCameraBtn.classList.add('btn-danger');
        switchCameraBtn.disabled = cameras.length <= 1;
        
        showToast('Camera ƒë√£ ƒë∆∞·ª£c b·∫≠t', 'success');
        
      } catch (error) {
        console.error('L·ªói khi b·∫≠t camera:', error);
        showToast('Kh√¥ng th·ªÉ truy c·∫≠p camera', 'error');
        
        // Kh√¥i ph·ª•c UI
        startCameraBtn.innerHTML = '<i class="fas fa-play"></i> B·∫≠t Camera';
        startCameraBtn.classList.remove('btn-danger');
        startCameraBtn.classList.add('btn-secondary');
      }
    }

    // D·ª´ng camera
    function stopCamera() {
      if (currentStream) {
        currentStream.getTracks().forEach(track => track.stop());
        currentStream = null;
      }
      
      videoElement.srcObject = null;
      videoElement.style.display = 'none';
      cameraPlaceholder.style.display = 'block';
      cameraStatus.style.display = 'none';
      
      // Kh√¥i ph·ª•c UI
      startCameraBtn.innerHTML = '<i class="fas fa-play"></i> B·∫≠t Camera';
      startCameraBtn.classList.remove('btn-danger');
      startCameraBtn.classList.add('btn-secondary');
      switchCameraBtn.disabled = true;
    }

    // Chuy·ªÉn ƒë·ªïi camera
    function switchCamera() {
      if (cameras.length <= 1) return;
      
      currentCameraIndex = (currentCameraIndex + 1) % cameras.length;
      const selectedCamera = cameras[currentCameraIndex];
      
      cameraSelect.value = currentCameraIndex;
      startCamera(selectedCamera.deviceId);
      
      showToast(`ƒê√£ chuy·ªÉn sang ${selectedCamera.label || `Camera ${currentCameraIndex + 1}`}`, 'warning');
    }

    // To√†n m√†n h√¨nh
    function toggleFullscreen() {
      const cameraFrame = document.querySelector('.camera-frame');
      
      if (!document.fullscreenElement) {
        if (cameraFrame.requestFullscreen) {
          cameraFrame.requestFullscreen();
        } else if (cameraFrame.webkitRequestFullscreen) {
          cameraFrame.webkitRequestFullscreen();
        } else if (cameraFrame.msRequestFullscreen) {
          cameraFrame.msRequestFullscreen();
        }
      } else {
        if (document.exitFullscreen) {
          document.exitFullscreen();
        } else if (document.webkitExitFullscreen) {
          document.webkitExitFullscreen();
        } else if (document.msExitFullscreen) {
          document.msExitFullscreen();
        }
      }
    }

    // Hi·ªÉn th·ªã th√¥ng b√°o
    function showToast(message, type = 'success') {
      toast.textContent = message;
      toast.className = 'toast';
      toast.classList.add(type);
      toast.classList.add('show');
      
      setTimeout(() => {
        toast.classList.remove('show');
      }, 3000);
    }

    // C·∫≠p nh·∫≠t hi·ªÉn th·ªã PIN
    function updatePinDisplay() {
      const displayValue = '*'.repeat(pinCode.length);
      pinDisplay.textContent = displayValue || '****';
    }

    // X·ª≠ l√Ω thu th·∫≠p khu√¥n m·∫∑t
    async function collectFaceData() {
      const userId = document.getElementById('userId').value.trim();
      const userName = document.getElementById('userName').value.trim();
      
      // Validation
      if (!userId || !userName) {
        showToast('Vui l√≤ng nh·∫≠p ƒë·∫ßy ƒë·ªß th√¥ng tin ng∆∞·ªùi d√πng', 'error');
        return;
      }
      
      if (pinCode.length !== maxPinLength) {
        showToast('Vui l√≤ng nh·∫≠p m√£ PIN x√°c th·ª±c (4 ch·ªØ s·ªë)', 'error');
        return;
      }
      
      if (!currentStream) {
        showToast('Vui l√≤ng b·∫≠t camera tr∆∞·ªõc khi thu th·∫≠p', 'error');
        return;
      }
      
      // Hi·ªáu ·ª©ng loading
      const originalText = collectBtn.innerHTML;
      collectBtn.innerHTML = '<i class="fas fa-spinner spinner"></i> ƒêang thu th·∫≠p...';
      collectBtn.disabled = true;
      
      try {
        // Ch·ª•p ·∫£nh t·ª´ video
        const canvas = document.createElement('canvas');
        const context = canvas.getContext('2d');
        canvas.width = videoElement.videoWidth;
        canvas.height = videoElement.videoHeight;
        context.drawImage(videoElement, 0, 0, canvas.width, canvas.height);
        
        const imageData = canvas.toDataURL('image/jpeg');
        
        // Gi·∫£ l·∫≠p g·ª≠i d·ªØ li·ªáu ƒë·∫øn server
        await new Promise(resolve => setTimeout(resolve, 2000));
        
        showToast(`ƒê√£ thu th·∫≠p khu√¥n m·∫∑t th√†nh c√¥ng cho ${userName}`, 'success');
        
        // Chuy·ªÉn h∆∞·ªõng sau khi th√†nh c√¥ng
        setTimeout(() => {
          window.location.href = `/face/collect?userId=${userId}&userName=${encodeURIComponent(userName)}&pin=${pinCode}`;
        }, 1500);
        
      } catch (error) {
        console.error('L·ªói khi thu th·∫≠p:', error);
        showToast('L·ªói khi thu th·∫≠p khu√¥n m·∫∑t', 'error');
      } finally {
        collectBtn.innerHTML = originalText;
        collectBtn.disabled = false;
      }
    }

    // Thi·∫øt l·∫≠p event listeners
    function setupEventListeners() {
      // Camera controls
      startCameraBtn.addEventListener('click', () => {
        if (currentStream) {
          stopCamera();
        } else {
          const selectedIndex = cameraSelect.value;
          if (selectedIndex) {
            startCamera(cameras[selectedIndex].deviceId);
          } else {
            startCamera();
          }
        }
      });
      
      cameraSelect.addEventListener('change', (e) => {
        if (e.target.value) {
          currentCameraIndex = parseInt(e.target.value);
          startCamera(cameras[currentCameraIndex].deviceId);
        }
      });
      
      switchCameraBtn.addEventListener('click', switchCamera);
      fullscreenBtn.addEventListener('click', toggleFullscreen);
      refreshBtn.addEventListener('click', loadCameras);
      
      // PIN buttons
      document.querySelectorAll('.pin-btn').forEach(button => {
        button.addEventListener('click', function() {
          const value = this.getAttribute('data-pin');
          
          if (value === 'clear') {
            pinCode = '';
            updatePinDisplay();
          } else if (value === 'enter') {
            if (pinCode.length === maxPinLength) {
              showToast(`M√£ PIN ${pinCode} ƒë√£ ƒë∆∞·ª£c x√°c th·ª±c`, 'success');
            } else {
              showToast('Vui l√≤ng nh·∫≠p ƒë·ªß 4 ch·ªØ s·ªë PIN', 'error');
            }
          } else if (pinCode.length < maxPinLength) {
            pinCode += value;
            updatePinDisplay();
          }
        });
      });
      
      // Collect button
      collectBtn.addEventListener('click', collectFaceData);
      
      // Input focus effects
      document.querySelectorAll('input').forEach(input => {
        input.addEventListener('focus', function() {
          this.style.borderColor = 'var(--primary-color)';
        });
        
        input.addEventListener('blur', function() {
          if (!this.value) {
            this.style.borderColor = 'var(--medium-gray)';
          }
        });
      });
      
      // Fullscreen change event
      document.addEventListener('fullscreenchange', () => {
        if (document.fullscreenElement) {
          fullscreenBtn.innerHTML = '<i class="fas fa-compress"></i> Tho√°t to√†n m√†n h√¨nh';
        } else {
          fullscreenBtn.innerHTML = '<i class="fas fa-expand"></i> To√†n m√†n h√¨nh';
        }
      });
    }
  </script>
